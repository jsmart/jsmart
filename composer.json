{
    "name": "jsmart/jsmart",
    "description": "JSmart Framework",
    "keywords": ["jsmart", "framework"],
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Vadim Shestakov",
            "email": "shestakov2103@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "application",
            "App\\Modules\\": "application/modules"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always"
    },
    "require": {
        "jsmart/framework": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
